{
  "name": "solefi-cms",
  "version": "1.0.0",
  "description": "Solefi CMS",
  "main": "manage.py",
  "scripts": {
    "start": "python3 manage.py runserver 0.0.0.0:8000",
    "static": "python3 manage.py collectstatic --noinput",
    "user": "./utils/createsuperuser.sh",
    "app": "./utils/createapp.sh",
    "migrations": "python3 manage.py makemigrations",
    "migrate": "python3 manage.py migrate",
    "freeze": "python3 -m pip freeze -l > requirements.txt",
    "install": "./deploy/install.sh",
    "uninstall": "./deploy/uninstall.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christopherigm/solefi-cms.git"
  },
  "author": "Christopher Guzman",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/christopherigm/solefi-cms/issues"
  },
  "homepage": "https://github.com/christopherigm/solefi-cms#readme"
}
